{
  "name": "lasota/laravel-content",
  "description": "Grab content from content folder in the same way laravels config grabs from config",
  "keywords": [
    "laravel",
    "content"
  ],
  "homepage": "https://github.com/spatie/url",
  "license": "MIT",
  "authors": [
    {
      "name": "Rob LaSota",
      "email": "rob.lasota@gmail.com",
      "homepage": "https://github.com/lasota",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "erusev/parsedown-extra": "^0.8.1",
    "laravel/framework": "^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Lasota\\LaravelContent\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lasota\\LaravelContent\\LaravelContentServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
